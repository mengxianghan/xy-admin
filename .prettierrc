{
    "tabWidth": 4,
    "bracketSameLine": true,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 120,
    "endOfLine": "auto",
    "singleAttributePerLine": true,
    "importOrder": ["^[a-zA-Z]/(.*)$", "^@[a-zA-Z](.*)$", "^@/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
