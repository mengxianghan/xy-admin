{
    "name": "xy-admin",
    "private": true,
    "version": "2.6.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:test": "vite build --mode test",
        "build:pre": "vite build --mode pre",
        "build:prod": "vite build --mode prod",
        "preview": "npm run build:prod && vite preview",
        "report": "vite build --mode prod",
        "lint": "eslint --config .eslintrc.cjs --ext .js,.vue --no-cache --fix src",
        "prettier": "prettier --config .prettierrc.cjs --write src/*.{js,vue} src/**/*.{js,vue}",
        "prepare": "husky install",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress build docs && vitepress preview docs"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "npm run prettier",
            "npm run lint"
        ]
    },
    "dependencies": {
        "@ant-design/colors": "^7.0.0",
        "@ant-design/icons-vue": "^7.0.1",
        "@icon-park/vue-next": "^1.4.2",
        "@tinymce/tinymce-vue": "^5.1.1",
        "ant-design-vue": "^4.0.7",
        "axios": "^1.6.2",
        "cropperjs": "^1.6.1",
        "dayjs": "^1.11.10",
        "echarts": "^5.4.3",
        "filesize": "^10.1.0",
        "filesize-parser": "^1.5.0",
        "jschardet": "^3.0.0",
        "json-bigint": "^1.0.0",
        "lodash-es": "^4.17.21",
        "nanoid": "^5.0.4",
        "nprogress": "^0.2.0",
        "overlayscrollbars": "^2.4.5",
        "overlayscrollbars-vue": "^0.5.6",
        "pinia": "^2.1.7",
        "qrcode": "^1.5.3",
        "sortablejs": "^1.15.1",
        "tinymce": "^6.8.2",
        "vue": "^3.3.12",
        "vue-router": "^4.2.5",
        "xy-enum": "^1.4.4",
        "xy-http": "^1.0.0",
        "xy-storage": "^3.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@vitejs/plugin-vue": "^4.5.2",
        "commitlint": "^18.4.3",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-vue": "^9.19.2",
        "husky": "^8.0.3",
        "less": "^4.2.0",
        "lint-staged": "^15.2.0",
        "rollup-plugin-visualizer": "^5.11.0",
        "vite": "^5.0.10",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-progress": "^0.0.7",
        "vitepress": "^1.0.0-beta.7"
    }
}
