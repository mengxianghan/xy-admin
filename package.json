{
  "name": "xy-admin",
  "private": true,
  "version": "2.5.1",
  "scripts": {
    "dev": "vite --mode dev",
    "build:test": "vite build --mode test",
    "build:pre": "vite build --mode pre",
    "build:prod": "vite build --mode prod",
    "preview": "npm run build:prod && vite preview --mode prod",
    "report": "vite build --mode prod",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore --fix src",
    "prepare": "husky install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress build docs && vitepress preview docs"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": "eslint --ext .js,.vue .eslintignore --no-cache --fix"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@tinymce/tinymce-vue": "^5.1.0",
    "ant-design-vue": "^3.2.16",
    "axios": "^0.27.2",
    "cropperjs": "^1.5.12",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "filesize": "^10.0.6",
    "filesize-parser": "^1.5.0",
    "jschardet": "^3.0.0",
    "json-bigint": "^1.0.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nanoid": "^4.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.30",
    "qiniu-js": "^3.4.1",
    "qrcode": "^1.5.1",
    "sortablejs": "^1.15.0",
    "tinymce": "^6.4.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "xy-enum": "^1.2.0",
    "xy-qiniu": "^1.0.0",
    "xy-storage": "^3.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "vite": "^4.1.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-progress": "^0.0.6",
    "vitepress": "^1.0.0-alpha.65"
  }
}
